<!-- Bootstrap core JS-->
<script src="./assets/js/bootstrap.bundle.min.js"></script>
<!-- Core theme JS-->
<script src="./assets/js/scripts.js"></script>
<!-- show message to user-->
<% if(typeof success !== "undefined" && typeof message !== "undefined"){ %>
    <%  if(success) { %>
        <script>
            document.addEventListener('DOMContentLoaded', () => {
                toastr.success('<%= message %>')
            })
        </script>
        <%   }else{ %>
            <script>
                document.addEventListener('DOMContentLoaded', () => {
                    toastr.error('<%= message %>')
                })
            </script>
    <%   } %>
<% } %>

<!-- show message to user from session-->
<% if(typeof sessionData !== "undefined" && typeof sessionData.success !== "undefined" && typeof sessionData.message !== "undefined"){ %>
    <%  if(sessionData.success) { %>
        <script>
            document.addEventListener('DOMContentLoaded', () => {
                toastr.success('<%= sessionData.message %>')
            })
        </script>
        <%   }else{ %>
            <script>
                document.addEventListener('DOMContentLoaded', () => {
                    toastr.error('<%= sessionData.message %>')
                })
            </script>
    <%   } %>
<% } %>

<!-- toastr -->
<script src="assets/toastr/toastr.min.js"></script>